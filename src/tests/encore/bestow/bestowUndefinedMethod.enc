local class IntBox
   var bar : int

   def init(bar : int) : unit
      this.setInt(bar)
   end
end

active class Foo
   var box : IntBox

   def init(bar : int) : unit
      this.box = new IntBox(bar)
   end

   def bestowBox() : bestowed[IntBox]
      bestow(this.box)
   end
end

active class Main
   def main() : unit
      var b = new Foo(7)
      var box = get(b ! bestowBox())
      box ! foo()
   end
end
