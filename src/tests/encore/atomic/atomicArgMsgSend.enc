active class Foo : Actor
  var i : int

  def init(i : int) : unit
     this.i = i
     println(this.i)
  end

  def getInt() : int
     this.i
  end
  
  def setInt(i : int) : unit
     this.i = i
     println(this.i)
  end

  def dummy() : int
     42
  end
end

active class Main          
  def main() : unit
     var foo = new Foo(11)
     atomic foo as bar in
       bar ! setInt(bar ! dummy())
     end
  end
end
